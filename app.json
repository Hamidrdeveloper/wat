    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "requireFullScreen": true,
      "supportsTablet": true,
      "isTabletOnly": true,
      "infoPlist": {
        "UISupportedInterfaceOrientations": [
          "UIInterfaceOrientationLandscapeRight",
          "UIInterfaceOrientationLandscapeLeft",
          "UIInterfaceOrientationPortrait"
      ],
      "UISupportedInterfaceOrientations~ipad": [
        "UIInterfaceOrientationLandscapeRight",
        "UIInterfaceOrientationLandscapeLeft",
        "UIDeviceOrientationPortrait",
        "UIDeviceOrientationPortraitUpsideDown"
      ]
      },
      "bundleIdentifier": "com.react.watchlist.watchlistPro",
      "buildNumber": "1.4.0",
      "config": {
        "googleMapsApiKey": "AIzaSyB3535z4eKMYIMwZZPPiXpd7UkhmnBT7HA"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "13.0"
          }
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "LANDSCAPE"
        }
      ]
    ],
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0133aa",
        "foregroundImage": "./assets/images/lo.png"
      },
      "package": "com.react.watchlist.watchlistPro",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCu0_rNhV2wN8CGmZR_C2C9ObybaLKI61k"
        }
      }
    },
    "web": {
      "favicon": "./assets/images/playstore.png"
    },
    "extra": {
      "eas": {
        "projectId": "4410f14f-f5d7-4a90-aa1d-70afaf08cd61"
      }
    }
  }
}
