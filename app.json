{
  "expo": {
    "name": "Watchlist - Publity",
    "slug": "Watchlist",
    "version": "19.0.0",
    "orientation": "landscape",
    "icon": "./assets/images/lo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
